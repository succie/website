{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "typeRoots": ["types", "node_modules/@types"]
  },
  "include": ["src", "types"]
}
